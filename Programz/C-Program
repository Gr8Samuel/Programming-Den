#include <stdio.h>

int main()
{
	int x;
	x = 10;
	int y;
	y = x / 2;
	int z = x - y * 2;

	printf("The value of x is: %i\n,The value of y is:%i\n,The value of z is: %i\n, x, y, z);
	return 0;
}
